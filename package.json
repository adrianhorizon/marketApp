    "@angular/language-service": "~7.2.0",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.14.2",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
