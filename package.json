{
  "name": "market-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run server\" \"ng serve --open\"",
    "start:yarn": "concurrently \"ng serve --open\" \"yarn server\"",
    "build": "ng build --prod",
    "test": "ng test",
    "server": "concurrently \"npm run server-1\" \"npm run server-2\"",
    "server-1": "json-server --watch server/categories.json ",
    "server-2": "json-server --watch server/products.json --port 3001",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@ngrx/effects": "^7.4.0",
    "@ngrx/entity": "^7.4.0",
    "@ngrx/router-store": "^7.4.0",
    "@ngrx/store": "^7.4.0",
    "@ngrx/store-devtools": "^7.4.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngx-pagination": "^3.2.1",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
